{
  "name": "denormalizr",
  "version": "0.1.0",
  "description": "State denormalization for normalizr",
  "main": "dist/denormalizr.js",
  "browser": "dist/denormalizr.js",
  "jsnext:main": "src/denormalizr.js",
  "scripts": {
    "test": "mocha --compilers js:babel-register test/*.js",
    "build": "rollup -f umd -n denormalizr -o dist/denormalizr.js -- ./src/denormalizr.js",
    "prepublish": "npm run test && npm run build"
  },
  "author": "Nicholas Husher <nhusher@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "babel-eslint": "^4.1.6",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-es2015-loose": "^7.0.0",
    "babel-register": "^6.7.2",
    "eslint": "^1.10.3",
    "lodash": "^4.6.1",
    "mocha": "^2.2.1",
    "normalizr": "^2.0.0",
    "rollup": "^0.35.10",
    "uglify-js": "^2.7.3"
  }
}
